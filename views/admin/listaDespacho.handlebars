<h4>Lista de Despachos</h4>
<br>
{{#each despachos}}

    <div id="{{_id}}" class="card mt-4">
        <div class="card-body">
            <div>
                <h6>Despacho: {{NprocessoDespacho}}</h6>
            </div>
            <div class="">
                <label class="mt-2">Data: {{despachoDataPedido}}</label>
                <a class="btn btn-success float-end" href="/formulario/formulario/despachoVizu/{{_id}}">Visualizar</a>
            </div>
        </div>
    </div>
    <script>
            horaLocal = Date.now()
            dezHoras = 36000000
            segundos = 20000

            if(horaLocal > {{despachoData}} + dezHoras) {
                console.log('a')
                const pegar = document.getElementById('{{_id}}').setAttribute('class', 'card mt-4 bg-danger')
            }else if(horaLocal > {{despachoData}} + segundos) {
                console.log('b')
                const pegar = document.getElementById('{{_id}}').setAttribute('class', 'card mt-4 bg-warning')
            } else {

            }
</script>
{{else}}
<h6>Nenhum despaho</h6>
{{/each}}

