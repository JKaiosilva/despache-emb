
<div class="card">
    <div class="card-body">
        <form action="/admin/users/addAgencia" method="POST">
            <input name="id" value="{{usuario._id}}" hidden>
            <label for="nome"> Nome </label>
                <input name="nome" type="text" value="{{usuario.nome}}" class="form-control">
            <label for="email">Email: </label>
                <input name="email" type="email" value="{{usuario.email}}" class="form-control">
            <label>CNPJ: </label>
                <input name="CPF" maxlength="14" type="text"  class="form-control" placeholder="Coloque os 14 dígitos sem pontos ou espaços">
            <br>
            <br>
                <div id="infosAgencia">
                    <span>Informações Necessárias para conta de Agencia</span>
                        <div class="input-group">
                            <label class="input-group-text">Proprietário</label>
                            <input name="proprietario" class="form-control" value="{{usuário.proprietario}}">
                        </div>
                        <br>
                        <div class="card">
                            <div id="selecaoSocios" class="input-group">
                                <div class="container">
                                    <h4 class="text-center">Lista de sócios proprietários</h4>
                                    {{>_socios}}
                                </div>
                            </div>
                        </div>

                        <br>
                </div>
                    <label type="senha">Senha: </label>
                        <input minlength="6" placeholder="A senha precisa ter no mínimo 6 dígitos" type="password" name="senha" class="form-control" required>
                    <label for="senha2">Repita sua senha: </label>
                        <input minlength="6" type="password" name="senha2" class="form-control" required>
            <button type="submit" id="submitButton" class="btn btn-success mt-4" disabled>Cadastrar</button>
        </form>
    </div>
</div>
    <br>
<input class="btn btn-success" type="button" value="Voltar" style="float: left" onClick="history.go(-1)"> 
