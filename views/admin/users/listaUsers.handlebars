<h2>Lista de usuários</h2>
<hr>
{{#each usuarios}}
    <div class="card mt-4">
        <div class="card-body">
            <h4>Nome: {{nome}}</h4>
            <h5>Email: {{email}}</h5>
            <label class="mt-2">Acesso de administrador: {{eAdmin}}</label>
            <a class="btn btn-success float-end" href="/admin/users/usuariosVizu/{{_id}}">Visualizar</a>
            <a class="btn btn-warning float-end mx-1" href="/admin/users/usuariosEdit/{{_id}}">Editar</a>
        </div>
    </div>
{{else}}
    <h4>Nenhum usuário cadastrado</h4>
{{/each}}
<br>
<a href="/admin/painel" class="btn btn-primary">Painel</a>
<a href="/admin/listaUsers/2" class="btn btn-primary" style="float: right">Próximo</a>
