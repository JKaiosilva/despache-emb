{{#each avisoSaidas}}
    <div id="{{_id}}" class="card mt-4">
        <div class="card-header">
            <label class="float-end">Situação: {{editado}}</label>
            <h6>Aviso de Saída: {{saidaNprocesso}}</h6>
        </div>
        <div class="card-body">
            <div>
                <label>Agência: {{agenciaNome}}</label>
                <br>
                <label>Embarcação: {{embarcacaoNome}}</label>
                <br>
                <label>Data Solicitada: {{saidaDataPedido}}</label>
                <br>
            </div>
        </div>
        <div class="card-footer">
            <label>Data Validade: {{saidaDataValidade}}</label>
            <a class="btn btn-success float-end" href="/formulario/avisoSaidaVizu/{{_id}}">Visualizar</a>
        </div>
    </div>
{{>_classificacao}}
{{else}}
<h6>Nenhuma Requisição de Saída</h6>
{{/each}}