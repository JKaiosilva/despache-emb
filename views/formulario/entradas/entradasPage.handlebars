            if(horaLocal > {{entradaData}} + dezHoras && {{../admin}}) {
                console.log('a')
                const pegar = document.getElementById('{{_id}}').setAttribute('class', 'card mt-4 bg-danger')
            }else if(horaLocal > {{entradaData}} + segundos && {{../admin}}) {
                console.log('b')
                const pegar = document.getElementById('{{_id}}').setAttribute('class', 'card mt-4 bg-warning')
            } else {
            }
</script>
{{else}}
